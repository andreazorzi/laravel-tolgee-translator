{
    "name": "andreazorzi/laravel-tolgee-translator",
    "version": "1.0.4",
    "license": "MIT",
    "description": "Integrate Tolgee with your Laravel project",
    "keywords": [
        "Andrea Zorzi",
        "laravel",
        "laravel-tolgee-translator",
        "tolgee"
    ],
    "homepage": "https://github.com/andreazorzi/laravel-tolgee-translator",
    "autoload": {
        "psr-4": {
            "LaravelTolgeeTranslator\\": "src/"
        },
        "files": [
            "src/Helpers/TolgeeHelper.php"
        ]
    },
    "authors": [
        {
            "name": "Andrea Zorzi",
            "email": "info@zorziandrea.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "spatie/laravel-package-tools": "^1.18",
        "laravel/framework": "^11.37"
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelTolgeeTranslator\\LaravelTolgeeTranslatorServiceProvider"
            ],
            "aliases": {
                "LaravelTolgeeTranslator": "LaravelTolgeeTranslator\\Facades\\LaravelTolgeeTranslator"
            }
        }
    }
}
